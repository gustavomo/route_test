{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./build",
    "removeComments": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": false,
    "esModuleInterop": true
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}